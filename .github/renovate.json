{
    "labels": [
        "chore"
    ],
    "packageRules": [
        {
            "groupName": "nuget",
            "description": "Dotnet nuget packages",
            "matchDatasources": [
                "nuget"
            ],
            "matchPackagePatterns": [
                ".*"
            ]
        },
        {
            "groupName": "Dockerfile updates",
            "matchDatasources": [
                "docker"
            ],
            "matchFileNames": [
                "**/Dockerfile"
            ]
        }
    ]
}
